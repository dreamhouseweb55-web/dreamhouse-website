{
    "compilerOptions": {
        "target": "es2016",
        "module": "es2015",
        "moduleResolution": "node",
        "outDir": "./_site/js",
        "rootDir": "./src/ts",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "DOM",
            "ES2016"
        ]
    },
    "include": [
        "src/ts/**/*"
    ]
}